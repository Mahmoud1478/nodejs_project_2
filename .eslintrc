{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-var" :"error"
    },
    "extends" :[
        "plugin:@typescript-eslint/recommended",
        "prettier",
        "plugin:prettier/recommended",
        
    ],
    "env": {
        "node": true,
        "es2021": true
      },
}